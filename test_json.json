{
  "pipeline": {
    "name": "json-test",
    "tasks": [
      {
        "id": "read",
        "type": "read_text",
        "path": "./test_input.txt"
      },
      {
        "id": "transform",
        "type": "transform",
        "plugin": "builtin:lowercase",
        "input": "@read.text"
      },
      {
        "id": "write",
        "type": "write_text",
        "path": "./json_out.txt",
        "input": "@transform.text"
      }
    ]
  }
}
